

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<div id="main-signup">
	<h2 style="text-align: center;">Sign up</h2>
	<div id="signup">
		<%= f.label :testing, style: "margin-left: 100px;" %><br />
		<%= f.email_field :email, autofocus: true, style: "height: 40px; width: 250px; margin-left: 0px; border-radius: 6px; border: 1px solid lightgrey;" %>
	</div>

	<div id="signup">
		<%= f.label :password %><br />
		<%= f.password_field :password, autocomplete: "off" %>
	</div>

	<div id="signup">
		<%= f.label :password_confirmation %><br />
		<%= f.password_field :password_confirmation, autocomplete: "off" %>
	</div>

	<div id="signup">
		<%= f.submit "Sign Up" %>
	</div>
	
	<% end %>



</div>